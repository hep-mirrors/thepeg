// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the MEQQ2qq class.
//

namespace ThePEG {

inline MEQQ2qq::MEQQ2qq() {}

inline MEQQ2qq::MEQQ2qq(const MEQQ2qq & x)
  : ME2to2QCD(x) {}

inline double MEQQ2qq::colA() const {
  return (sqr(uHat()) + sqr(tHat()))/sqr(sHat());
}

inline IBPtr MEQQ2qq::clone() const {
  return new_ptr(*this);
}

inline IBPtr MEQQ2qq::fullclone() const {
  return new_ptr(*this);
}

}
