namespace ThePEG {

inline MadGraphTwoCut::MadGraphTwoCut()
: cutType(DELTAR), pairType(JETJET), theCut(0.0) {}

inline MadGraphTwoCut::MadGraphTwoCut(CutType t, PPType p, double c)
: cutType(t), pairType(p), theCut(c) {}

inline MadGraphTwoCut::MadGraphTwoCut(const MadGraphTwoCut & x)
  : TwoCutBase(x), cutType(x.cutType), pairType(x.pairType), theCut(x.theCut) {}

inline IBPtr MadGraphTwoCut::clone() const {
  return new_ptr(*this);
}

inline IBPtr MadGraphTwoCut::fullclone() const {
  return new_ptr(*this);
}

