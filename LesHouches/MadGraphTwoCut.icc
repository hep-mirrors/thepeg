inline void MadGraphTwoCut::doinit() throw(InitException) {
  TwoCutBase::doinit();
}

inline void MadGraphTwoCut::dofinish() {
  TwoCutBase::dofinish();
}

inline void MadGraphTwoCut::doinitrun() {
  TwoCutBase::doinitrun();
}

inline void MadGraphTwoCut::rebind(const TranslationMap & trans)
  throw(RebindException) {
  // dummy = trans.translate(dummy);
  TwoCutBase::rebind(trans);
}

inline IVector MadGraphTwoCut::getReferences() {
  IVector ret = TwoCutBase::getReferences();
  // ret.push_back(dummy);
