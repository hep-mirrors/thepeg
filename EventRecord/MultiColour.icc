// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the MultiColour class.
//

namespace ThePEG {

inline MultiColour::MultiColour() {}

inline MultiColour::MultiColour(const MultiColour & x)
  : ColourBase(x), theColourLines(x.theColourLines),
    theAntiColourLines(x.theAntiColourLines) {}

inline EIPtr MultiColour::clone() const {
  return new_ptr(*this);
}

}
