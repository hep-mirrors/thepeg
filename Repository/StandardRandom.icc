namespace ThePEG {

inline StandardRandom::StandardRandom()
  : u(97) {
  setSeed(theSeed);
}

inline StandardRandom::StandardRandom(const StandardRandom & x)
  : RandomGenerator(x), u(x.u), c(x.c), cd(x.cd), cm(x.cm),
    i97(x.i97), j97(x.j97) {}

}

