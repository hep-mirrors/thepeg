inline void Onium3GDecayer::doinit() throw(InitException) {
  FlatDecayer::doinit();
}

inline void Onium3GDecayer::dofinish() {
  FlatDecayer::dofinish();
}

inline void Onium3GDecayer::doinitrun() {
  FlatDecayer::doinitrun();
}

inline void Onium3GDecayer::rebind(const TranslationMap & trans)
  throw(RebindException) {
  // dummy = trans.translate(dummy);
  FlatDecayer::rebind(trans);
}

inline IVector Onium3GDecayer::getReferences() {
  IVector ret = FlatDecayer::getReferences();
  // ret.push_back(dummy);
  return ret;
