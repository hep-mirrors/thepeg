namespace ThePEG {

inline Onium3GDecayer::Onium3GDecayer()
: doShower(true), theMinGGMass(2.0*GeV) {}

inline Onium3GDecayer::Onium3GDecayer(const Onium3GDecayer & x)
  : FlatDecayer(x), doShower(x.doShower), theMinGGMass(x.theMinGGMass) {}

inline IBPtr Onium3GDecayer::clone() const {
  return new_ptr(*this);
}

inline IBPtr Onium3GDecayer::fullclone() const {
  return new_ptr(*this);
}

inline bool Onium3GDecayer::shower() const {
  return doShower;
}

inline Energy Onium3GDecayer::minGGMass() const {
  return theMinGGMass;
}

