// class from CLHEP, placing it in the <!id>CLHEP<!!id> namespace as
// <!id>Vector3<!!id> and making it available in the
// <!id>ThePEG<!!id> namespace.

#include "CLHEP.h"
#include "CLHEP/Vector/ThreeVector.h"

namespace CLHEP {

  typedef Hep3Vector Vector3;

}

namespace ThePEG {

  using CLHEP::Vector3;

}

#endif /* ThePEG_ThreeVector_FH */
