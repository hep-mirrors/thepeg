// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the Debug class.
//

namespace ThePEG {

inline bool Debug::debugItem(int item) {
  if ( level == noDebug ) return false;
  if ( level == full ) return true;
  return ( item < 0 || size_t(item) >= debugItems.size() )? false:
    debugItems[item];
}

}
